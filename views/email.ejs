CREATE TABLE users (
  id BIGSERIAL PRIMARY KEY,
  first_name VARCHAR(50),
  last_name VARCHAR(50),
  email VARCHAR(50),
  password VARCHAR(50),
  interval INTEGER,
  notification BOOLEAN
);

CREATE TABLE contacts (
  id BIGSERIAL PRIMARY KEY,
  owner_id INTEGER,
  contact_id INTEGER,
  nickname VARCHAR(50),
  interval INTEGER
);

